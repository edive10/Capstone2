<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StressLevelActivity">


    <TextView
        android:id="@+id/stressLevelText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stress Level: 0"
        android:textSize="18sp"
        android:textColor="#3A322F"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/someOtherElement"
        android:layout_marginTop="16dp"
        tools:ignore="HardcodedText,UnknownId" />

</androidx.constraintlayout.widget.ConstraintLayout>
